	name = Edvard Sire
	signingkey = 76A667AC7675A3E8

[commit]
	gpgsign = true

[alias]
  aliases = "!git config --global -l | grep ^alias | sed s/alias.// | sort"
  conflicts = diff --name-only --diff-filter=U --relative
  count = rev-list --left-right --count master...
  show-commits = rev-list --left-right --pretty=oneline master...
  tracked = ls-tree --name-only -r HEAD

[init]
	defaultBranch = main
